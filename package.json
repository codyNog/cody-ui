{
  "name": "@codynog/cody-ui",
  "version": "0.0.1",
  "module": "cli.ts",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.26.0",
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^2.0.2",
    "@octokit/types": "^13.8.0",
    "@storybook/addon-actions": "^8.5.3",
    "@storybook/addon-controls": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-ondevice-actions": "^8.5.4",
    "@storybook/addon-ondevice-backgrounds": "^8.5.4",
    "@storybook/addon-ondevice-controls": "^8.5.4",
    "@storybook/addon-ondevice-notes": "^8.5.4",
    "@storybook/core": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-native": "^8.5.4",
    "@storybook/react-native-web-vite": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@types/adm-zip": "^0.5.7",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^22.13.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.19.10",
    "cross-env": "^7.0.3",
    "cspell": "^8.17.3",
    "esbuild": "^0.25.0",
    "npm-check-updates": "^17.1.15",
    "npm-run-all": "^4.1.5",
    "plop": "^4.0.1",
    "storybook": "^8.5.5",
    "typescript": "~5.3.3",
    "vite": "^6.1.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^21.1.1",
    "adm-zip": "^0.5.16",
    "dotenv": "^16.4.7"
  },
  "bin": {
    "@codynog/cody-ui": "./dist/index.js"
  },
  "optionalDependencies": {
    "@codynog/ai-rules": "latest"
  },
  "scripts": {
    "extract": "echo 'Extract functionality removed as per user request'",
    "build": "tsc --project tsconfig.build.json && mv dist/cli.js dist/index.js",
    "type-check": "tsc --noEmit",
    "format": "biome check --write .",
    "postplop": "npm run format",
    "postinstall": "node -e \"try { require('@codynog/ai-rules'); } catch (e) { console.log('Optional dependency @codynog/ai-rules not available, skipping...'); }\"",
    "plop": "plop",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories --config-path .rnstorybook",
    "lint": "run-p lint:*",
    "lint:script": "npx biome check .",
    "lint:spell": "cspell . --no-progress",
    "build-check": "tsc --noEmit",
    "dev": "npm run storybook:web",
    "update": "npm-check-updates -u"
  }
}
